{
    "name": "antoniog85/warehouse",
    "description": "Warehouse microservice based on Lumen",
    "keywords": [
        "warehouse", "microservice", "rest", "api"
    ],
    "authors": [{
        "name": "Antonio Giuliana",
        "email": "job@antoniogiuliana.co.uk"
    }],
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
//        "antoniog85/mediatype": "*@dev",
        "predis/predis": "^1.1",
        "illuminate/redis": "^5.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Warehouse\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
